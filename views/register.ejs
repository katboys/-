     <script type="text/javascript">
      function check(){
        var username = $('#account')[0].value;
        var password = $('#password')[0].value;
        var rpassword = $('#rpassword')[0].value;
        var wechat = $('#wechatip')
        if(username.length>20||username.length<6){
          alert('用户名长度应当在6到20个字符之间')
        }
        if(password!=rpassword){
          alert('两次密码不相同，请重新输入!');
          return false;
        }
        if(password.length>20||password.length<6){
          alert('密码应当在6到20个字符之间！');
          return false;
        }
        if(wechat.length>20||wechat.length<1){
          alert('微信号长度在3到20个字符之间');
          return false;
        }
        return true;
      };
     </script>
